<?xml version="1.0" encoding="utf-8"?><Keyboard    xmlns:android="http://schemas.android.com/apk/res/android"    android:horizontalGap="0px"    android:keyHeight="12%p"    android:keyWidth="25%p"    android:verticalGap="0px"    >    <Row>        <Key            android:codes="8"            android:keyLabel="1"/>        <Key            android:codes="9"            android:keyLabel="2"/>        <Key            android:codes="10"            android:keyLabel="3"/>        <Key            android:codes="67"            android:isRepeatable="true"            android:keyEdgeFlags="right"            android:keyHeight="24%p"            android:keyIcon="@drawable/icon_delete_32dp"/>    </Row>    <Row>        <Key            android:codes="11"            android:keyLabel="4"/>        <Key            android:codes="12"            android:keyLabel="5"/>        <Key            android:codes="13"            android:keyLabel="6"/>    </Row>    <Row>        <Key            android:codes="14"            android:keyLabel="7"/>        <Key            android:codes="15"            android:keyLabel="8"/>        <Key            android:codes="16"            android:keyLabel="9"/>        <Key            android:codes="66"            android:keyEdgeFlags="right"            android:keyHeight="24%p"            android:keyLabel="@string/confirm"           />    </Row>    <Row>        <Key            android:codes="46"            android:keyLabel=""            />        <Key            android:codes="7"            android:keyLabel="0"/>        <Key            android:codes="6"            android:keyLabel="00"            />    </Row></Keyboard>