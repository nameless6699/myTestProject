<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="false">
        <layer-list>
            <item android:width="@dimen/dp_35" android:height="@dimen/dp_35">
                <shape android:shape="oval">
                    <!--                    <solid android:color="#b3536673"></solid>-->
                </shape>
            </item>

            <item android:left="@dimen/dp_2" android:right="@dimen/dp_2" android:top="@dimen/dp_2" android:bottom="@dimen/dp_2">
                <shape android:shape="oval">
                    <!--                    <size android:height="30dp" android:width="30dp"></size>-->
                    <solid android:color="@color/graye3"></solid>
                    <stroke android:width="1dp" android:color="@color/white"></stroke>
                </shape>
            </item>
        </layer-list>
    </item>

    <item android:state_checked="true">
        <layer-list>
            <item android:width="@dimen/dp_35" android:height="@dimen/dp_35">
                <shape android:shape="oval">
                    <!--                    <solid android:color="#b3536673"></solid>-->
                </shape>
            </item>

            <item android:left="@dimen/dp_2" android:right="@dimen/dp_2" android:top="@dimen/dp_2" android:bottom="@dimen/dp_2">
                <shape android:shape="oval">
                    <solid android:color="@color/graye3"></solid>
                    <stroke android:width="1dp" android:color="@color/white"></stroke>
                </shape>
            </item>
        </layer-list>
    </item>
</selector>